{
  "name": "chat-front",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "runandwatch": "concurrently \"vite\" \"npm run codegen --watch\"",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.11.1",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@fontsource/roboto": "^5.0.13",
    "@mui/icons-material": "^5.16.4",
    "@mui/material": "^5.16.4",
    "graphql": "^16.9.0",
    "graphql-ws": "^5.16.0",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.4",
    "object-path": "^0.11.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.25.1",
    "react-svg": "^16.1.34",
    "sort-by": "^0.0.2"
